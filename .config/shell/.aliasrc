# .aliasrc
alias ls="ls --color=auto --group-directories-first -XF"
alias la="ls --color=auto --group-directories-first -AXF"
alias ll="ls --color=auto --group-directories-first -lhXF"
alias lz="ls --color=auto --group-directories-first -lhAXF"
alias nv="nvim"
alias kpmyd="kpcli --no-recycle --histfile /dev/null --kdb $HOME/Documents/Data/zhong_data.kdbx"
alias grep="grep --color=auto"
alias vf="vifmrun"
alias vifm="vifmrun"
alias hc="herbstclient"
alias ip="ip -color=auto"
alias notify-send="dznotify"
alias sqlite3="sqlite3 -init "$XDG_CONFIG_HOME"/sqlite3/sqliterc"
